<h2>Modifier projet</h2>
<%= form_for(:this, url:  admin_edit_project_path, html: {class: "form" }) do |f| %>
<div class="form-group">
  <div class="col-sm-10">
    <%= f.text_field :name, autofocus: true , :class => 'input' ,:placeholder=>"Titre" %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-10">
    <%= f.text_area :description, :class => "tinymce", :rows => 40, :cols => 120 %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-10">
    <%= f.file_field :thumbmail ,:class => 'input' ,:placeholder=>"Lien de la photo du projet"%>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-10">
    <%= f.text_field :link, :class => "input", :placeholder => "Lien vers votre projet" %>
  </div>
</div>
<div class="form-group">
  <div class="col-sm-10">
    <%= f.submit "Mise Ã  jour", :class => 'input-btn' %>
  </div>
</div>
<% end %>